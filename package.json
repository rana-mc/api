    "axios": "^0.27.2",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.1",
    "http-proxy-middleware": "^2.0.6",
    "lowdb": "^3.0.0",
    "node-html-parser": "^5.3.3",
    "socket.io": "^4.5.0",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.6.4"
